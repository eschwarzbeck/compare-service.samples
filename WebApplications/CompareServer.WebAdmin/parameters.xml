<?xml version="1.0" encoding="utf-8" ?>
<parameters>
  <parameter name="Log folder location" description="Please provide the Compare Service log folder location"
            defaultValue="C:\Program Files\Workshare\Compare Service\logs" tags="">
    <parameterEntry kind="XmlFile" scope="\\web.config$" match="/configuration/appSettings/add[@key='LogsDirectory']/@value" />
  </parameter>
  <parameter name="Rendering set location" description=""
              defaultValue="C:\ProgramData\Workshare\Compare Service\Rendering Sets" tags="">
    <parameterEntry kind="XmlFile" scope="\\web.config$" match="/configuration/appSettings/add[@key='renderset.path']/@value" />
  </parameter>
  <parameter name="Endpoint Address" description=""
                defaultValue="http://localhost:8080/Comparer/Compare5" tags="">
    <parameterEntry kind="XmlFile" scope="\\web.config$" match="/configuration/appSettings/add[@key='EndpointAddress']/@value" />
  </parameter>
</parameters>